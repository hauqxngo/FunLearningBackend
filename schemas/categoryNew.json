{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.category.com/categoryNew.schema.json",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "handle": {
      "type": "string",
      "minLength": 1,
      "maxLength": 10
    },
    "description": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": ["name", "handle", "description"]
}
